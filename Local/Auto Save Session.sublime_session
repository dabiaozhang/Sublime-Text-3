tationName,$busNumber,$ticketDate,$ticketTime,$toStationName); //获取车次基础数据\n        if(!$busInfo){\n            $arrReturn['code']=0;\n            $arrReturn['message']='网络异常，请尝试重新点击提交订单！';\n            $arrReturn['return']='';\n            return $arrReturn;\n        }\n\n        $arrOrder['BookingWebsite']=$busInfo['book_info']['order_site'];\n\n        $arrOrder['OrderNumber']= $this->getOrderNumberByRef($arrData['ref']);\n        $arrOrder['OrderChannel']= $this->getOrderChannelByRef($arrData['ref']);\n\n        // 铁友分销参数\n        $arrOrder['UtmSource']= $utmSource;\n        // 携程分销参数\n        $arrOrder['AllianceID'] = intval($arrData['Union']['AllianceID']);\n        $arrOrder['SID'] = intval($arrData['Union']['SID']);\n        $arrOrder['OUID'] = '';\n        $dataExt = unserialize($busInfo['paramters']);\n        // OUID存放产品扩展信息\n        if (!empty($dataExt['tag'])) {\n        	$ouid = '';\n        	foreach ($dataExt['tag'] as $k => $v) {\n        		$ouid .= $v['name'].':'.$v['desc'].'|';\n        	}\n        	$ouid = substr($ouid, 0, -1);\n        	$arrOrder['OUID'] = $ouid;\n        }\n        \n        $arrOrder['SourceId'] = empty($arrData['SourceId']) ? '' : trim(addslashes($arrData['SourceId']));\n        //反序列化businfo的paramters节点  获取车次相关的信息  车型，车牌等\n        $carInfo=unserialize($busInfo['paramters']);\n        //司机手机号码 落地到VendorId\n        $arrOrder['VendorId']  =  $carInfo['busInfo']['driver_tel'];\n        //小车的车票号 落地到FetcherDesc\n        $arrOrder['FetcherDesc']  =  $carInfo['busInfo']['bus_credit_card'];\n\n        $arrTicketInfo = array();\n        $objPassenger = new passenger();\n\n        //乘客信息\n        for ($i = 1; $i <= $intIdentityInfoCount; $i++) {\n            $identityInfo = explode(';', $arrData['identityInfo'.$i]);\n            $ticketInfo['FromCityName'] = $fromCityName;\n            $ticketInfo['ToCityName'] = $toCityName;\n            $ticketInfo['FromStationName'] = $fromStationName;\n            $ticketInfo['ToStationName'] = $toStationName;\n            $ticketInfo['TicketDate'] = $ticketDate;\n            $ticketInfo['TicketTime'] = $ticketTime;\n            $ticketInfo['BusNumber'] = $busNumber;\n            $ticketInfo['TicketPrice'] = $busInfo['full_price'];\n            $ticketInfo['IdentityName'] = $identityInfo[0]; // 乘客姓名\n            $ticketInfo['IdentityType'] = strtr($identityInfo[1], array('身份证' => '二代身份证')); // 证件类型\n\n            $ticketInfo['FromStationAddress']=$fromStationAddress;\n            //出发车站ID   下单必传\n            $ticketInfo['FromStationId']=5;\n            //上车点地址     下单必传\n            $ticketInfo['OnBoardAddress']=$fromStationAddress;\n\n            $ticketInfo['IdentityCode'] = $identityInfo[2]; // 证件号码\n            $ticketInfo['TicketType'] = '1';\n            $ticketInfo['BusType'] = $busType; // 车次类型\n            $ticketInfo['ToTime'] = $toTime; // 到达时间\n            $ticketInfo['Todays'] = $toDays; // 到达天数\n\n\n            $ticketInfo['ShiftType'] = intval($busInfo['shift_type']); // 班次类型\n            $arrTicketInfo[]=array('TicketItem'=>$ticketInfo);\n        }\n\n        $ticketCnt = count($arrTicketInfo);\n        $arrOrder['OrderTitle']= $ticketDate.\" \".$fromStationName.\"-\".$toCityName.\" 汽车票\".$ticketCnt.\"张\" ;\n        $arrOrder['TicketCnt']=$ticketCnt;\n        $arrOrder['MajorCnt']=$ticketCnt;\n        $arrOrder['ChildCnt']=0;\n\n        // 计算订单总金额 = 票价*乘客数 + 乘客数*（服务费/支付手续费/0） + 保险*乘客数\n\n        $busInfo['utmSource'] = $utmSource;\n        $busInfo['client'] = strpos($arrData['partner'],'h5') === false ? 'ctrip_app' : 'ctrip_h5';\n\n        //计算订单总金额 = 票价*乘客数\n        $totalTicketFee = $busInfo['full_price'] * $ticketCnt;\n        $arrOrder['ServiceFee'] = 0;\n\n        $arrOrder['TotalFee'] = $totalTicketFee;\n        $arrOrder['TicketFee']=$totalTicketFee;\n        $arrOrder['InsuranceFee'] = 0;\n        $arrOrder['TravelTicketFee'] = 0;\n        \n        if ($busInfo['service_fee'] > 0) {\n        	$arrOrder['ServiceFee'] = $busInfo['service_fee'] * $ticketCnt;\n        	$arrOrder['TotalFee'] = $arrOrder['TotalFee'] + $arrOrder['ServiceFee'];\n        }\n        \n        // 1元免单\n        if (!empty($arrData['couponId'])) {\n        	$oneFreeAmount = 0;\n        	$arrCouponId = explode('|', $arrData['couponId']);\n        	require_once('bus/control/config/class.hotelCoupon.php');\n        	$objHotelCoupon = new hotelCoupon();\n        	foreach ($arrCouponId as $key => $val) {\n        		if ($val == 5) {\n        			$arrFreeOrders = $objHotelCoupon->getFreeOrdersItemById($val);\n        			if(!empty($arrFreeOrders)){\n        				$arrOrder['XpackageInfo'][] = array('Item' => array(\n        						\"XProductType\" => 2,\n        						\"XProductId\" => 0,\n        						\"XProductTableName\" => \"tbl_order_for_free\",\n        						\"XProductTableParams\" => array(\n        								\"order_number\" => $arrOrder['OrderNumber'],\n        								\"pay_fee\" => $arrFreeOrders['amount'],\n        								\"product_name\" => $arrFreeOrders['name'],\n        								\"create_dttm\" => date(\"Y-m-d H:i:s\")\n        						),\n        						'XProductCountUnit' => 1\n        				));\n        			}\n        			if(!empty($arrFreeOrders['amount']) && $arrFreeOrders['amount'] > 0){\n        				$oneFreeAmount = $ticketCnt * $arrFreeOrders['amount'];\n        			}\n        		}\n        	}\n        \n        	$arrOrder['TotalFee'] = $arrOrder['TotalFee'] + $oneFreeAmount;\n        }\n        \n        // 加载保险\n        require_once('bus/control/config/class.insurance.php');\n        $obj_insurance = new insurance();\n        $arrServicePackageConfigItem = $obj_insurance->getAllInsurance();\n        \n        if (isset($arrData['selectServicePackage2'])) {\n        	require_once('bus/control/config/class.servicePackage.php');\n        	$objServicePackage = new servicePackage();\n        	$servicePackage = $objServicePackage->resultFileterById($arrServicePackageConfigItem, $arrData['selectServicePackage2']);\n        	//1和3均为 20元 10元保险\n        	if ($servicePackage['type'] == 'insuran') {\n        		$arrOrder['InsuranceFee'] = $servicePackage['amount'] * $ticketCnt;\n        	}\n        \n        	if ($servicePackage['type'] == 'travel') {\n        		$arrOrder['TravelTicketFee'] = TRAVEL_TICKET_FEE * $ticketCnt;\n        	}\n        }\n        $arrOrder['TotalFee'] = $arrOrder['TotalFee'] + $arrOrder['InsuranceFee'] + $arrOrder['TravelTicketFee'];\n        \n        $arrOrder['ClientType'] = !empty($arrData['clientType']) ? trim($arrData['clientType']) : trim($arrData['ClientType']);\n        $arrOrder['ClientInfo'] = trim($arrData['clientInfo']);\n        $arrOrder['ClientVersion'] = trim(addslashes($arrData['clientVersion']));\n        $arrOrder['ClientId'] = $arrData['head']['cid'];\n        $arrOrder['CanOrderCheckFlag']='N';\n        $arrOrder['BookType']=3;\n        $arrOrder['TicketInfo']=$arrTicketInfo;\n\n        $arrUserInfo['UserID']=$userId;\n        $arrUserInfo['UserName']=$userName;\n        $arrUserInfo['ContactName']=$contactName;\n        $arrUserInfo['ContactMobile']=$contactMobile;\n        $arrUserInfo['ContactEmail']=\"\";\n        $arrUserInfo['FetcherIdentity']=$contactName.\"|\".$contactPaperType.\"|\".$contactPaperNum;\n        $arrOrder['UserInfo']=$arrUserInfo;\n        $arrOrder['BookType']=3;\n        // return $arrOrder;\n        // 判断选择优惠券\n        $arrSelectUseCoupon = $this->selectUseCoupon($arrData, $arrOrder);\n        if ($arrSelectUseCoupon['code'] <= 0) {\n            // 验证使用优惠券不通过\n            return $arrSelectUseCoupon;\n        }\n\n        $pointOrderApi=new pointOrderApi();\n        $addResponse = $pointOrderApi->addPointOrder($arrOrder);\n\n        if($addResponse['Status']=='SUCCESS'){\n            if ($addResponse['OrderId']) {\n\n                //支付详情 start\n                #print_r($arrTicketInfo);\n                $osTickInfo = array();\n                foreach ($arrTicketInfo as $key => $value) {\n                    array_push($osTickInfo,array(\n                        'KOrderDetailMinorTitle'=>$value['TicketItem']['IdentityName'],\n                        'KOrderDetailMinorComment'=>$value['TicketItem']['IdentityType'].'：'.$value['TicketItem']['IdentityCode']\n                    ));\n                }\n\n                $orderSummary = array(\n                    'KOrderSummary'=>array(\n                        'KOrderMainTitle'=>'单程 '.$fromStationName.' - '.$toCityName,\n                        'KOrderMinorTitle'=>'出发：'.$ticketDate.' '.$ticketTime.' 汽车票余票变动频繁，请尽量在15分钟内完成支付，可提高出票成功率哦~',\n                    ),\n                    'KOrderDetail'=>array(\n                        array(\n                            'KOrderDetailMainTitle'=>'乘车人',\n                            'KOrderDetailMinor'=>$osTickInfo,\n                        )\n                    )\n                );\n\n\n                array_push($orderSummary['KOrderDetail'],array(\n                    'KOrderDetailMainTitle'=> '取票人',\n                    'KOrderDetailMainValue'=> $arrUserInfo['ContactName'],\n                    'KOrderDetailMainComment'=> $contactPaperType.'：'.$contactPaperNum\n                ));\n                array_push($orderSummary['KOrderDetail'],array(\n                    'KOrderDetailMainTitle'=> '联系电话',\n                    'KOrderDetailMainValue'=> $contactMobile,\n                ));\n\n                //支付详情 end\n                // 需要支付的金额\n                $intDisplayRealPayFee = $addResponse['DisplayRealPayFee']*1;\n                // 支付方式扩展参数\n                $arrPayExtend = $addResponse['PayExtendInfo'];\n                $arrReturn['return'] = array(\n                    'orderNumber' => $addResponse['OrderNumber'],\n                    'orderId' => $addResponse['OrderId'],\n                    'displayRealPayFee' => $intDisplayRealPayFee,\n                    'payExtendInfo' => $arrPayExtend,\n                    'orderSummary'=>json_encode($orderSummary)\n                );\n                $arrReturn['code']=1;\n                $arrReturn['message']='';\n                return $arrReturn;\n            } else {\n                $arrReturn['code']=0;\n                $arrReturn['message']='订单创建失败';\n                $arrReturn['return']='';\n                return $arrReturn;\n            }\n        }else{\n            $arrReturn['code']=0;\n            $arrReturn['message']=$addResponse['ErrorResponse']['ErrorMessage'];\n            $arrReturn['result']=$addResponse;\n            $arrReturn['return']=$arrOrder;\n            $arrReturn['userInfo']=$arrUserInfo;\n            return $arrReturn;\n        }\n\n\n\n    }\n\n    /**\n     * 查询订单详情api\n     *\n     * @param  string $strOrderNumber\n     * @return array\n     */\n    public function orderDetail($arrData,$response) {\n        //验证用户信息\n        $arrLoginUserInfo = $this->checkUserApi($arrData);\n        if ($arrLoginUserInfo['code'] != 1) {\n            return $arrLoginUserInfo;\n        }\n        if($arrLoginUserInfo['return']['loginType'] == 'GetOrder' && $response['Order']['UserInfo']['ContactMobile'] == $arrLoginUserInfo['return']['loginName']){\n            $arrLoginUserInfo['return']['userID'] = $response['Order']['UserInfo']['UserID'];\n        }\n\n\n\n        if($response['Status']=='SUCCESS'){\n            $objAuthApi = new authApi();\n            $userInfo = $objAuthApi->getUserInfoToH5(array('Auth'=>$arrData['head']['auth'],'BizType'=>'Other'));\n\n            // 判断是否是该用户的订单\n            if ($response['Order']['UserInfo']['UserID'] != $arrLoginUserInfo['return']['userID']) {\n                //接入公共防鉴权服务\n                $checkAuthResult = self::checkAuth($response['Order']);\n                //print_r($checkAuthResult);die;\n                if($checkAuthResult['ReturnCode'] != 0){\n                    $arrReturn['code'] = 0;\n                    $arrReturn['message'] = '';\n                    $arrReturn['return'] = $checkAuthResult;\n                    return $arrReturn;\n                }\n            }\n            $v = $response['Order'];\n            if (!isset($v['TicketInfo']['TicketItem'][0])) {\n                $v['TicketInfo']['TicketItem'] = array($v['TicketInfo']['TicketItem']);\n            }\n\n            $arrOrderDetail = array();\n            if(isset($v['VendorInfo']['Mark']) ){\n                $arrOrderDetail['vendor'] = $this->_objBusDataApi->getVendorInfo($v['VendorInfo']['Mark'],false);\n            }\n\n            // 判断是否会员\n            if(!empty($arrData['head']['auth']) && $arrData['head']['auth'] !='null'){\n                require_once('bus/control/api/class.authApi.php');\n                $objAuthApi = new authApi();\n                $objUserInfo = $objAuthApi->validateAndGetNewToken(array('Token' => $arrData['head']['auth']));\n                if($objUserInfo['LoginType'] == \"TempUser\" || $objUserInfo['LoginType'] == \"GetOrder\"){\n                    $arrOrderDetail['loginType'] = 2; // 临时会员\n                }else{\n                    $arrOrderDetail['loginType'] = 1; // 会员\n                }\n            }else{\n                $arrOrderDetail['loginType'] = 0;\n            }\n\n\n            $arrOrderDetail['orderNumber'] = $v['OrderNumber'];\n            $arrOrderDetail['orderState'] = $v['Status'];\n            $arrOrderDetail['TradeNo'] = $v['TradeNo'];\n            $arrOrderDetail['orderShowStyle'] = 1;\n            $arrOrderDetail['orderDateTime'] = substr($v['OrderTime'], 0, 10);\n            $arrOrderDetail['busNumber'] = $v['TicketInfo']['TicketItem'][0]['BusNumber'];\n            $arrOrderDetail['realBusNumber'] = $v['TicketInfo']['TicketItem'][0]['RealBusNumber'];\n\n            $arrOrderDetail['busType'] = !empty($v['TicketInfo']['TicketItem'][0]['BusType']) ? $v['TicketInfo']['TicketItem'][0]['BusType'] : '';\n            $arrOrderDetail['fromCityName'] = $v['TicketInfo']['TicketItem'][0]['FromCityName'];\n            $arrOrderDetail['toCityName'] = $v['TicketInfo']['TicketItem'][0]['ToCityName'];\n            $arrOrderDetail['fromStationName'] = $v['TicketInfo']['TicketItem'][0]['FromStationName'];\n            $arrOrderDetail['toStationName'] = $v['TicketInfo']['TicketItem'][0]['ToStationName'];\n            // 如果到达站为空则显示到达城市\n            if (empty($arrOrderDetail['toStationName'])) {\n                $arrOrderDetail['toStationName'] = $arrOrderDetail['toCityName'];\n            }\n            $arrOrderDetail['fromShowName'] = $arrOrderDetail['fromCityName'];\n            $arrOrderDetail['toShowName'] = $arrOrderDetail['toCityName'];\n            $arrOrderDetail['ticketDate'] = $v['TicketInfo']['TicketItem'][0]['TicketDate'];\n            $arrOrderDetail['realTicketDate'] = $v['TicketInfo']['TicketItem'][0]['RealFromDate'];\n            $arrOrderDetail['ticketFromTime'] = $v['TicketInfo']['TicketItem'][0]['TicketTime'];\n            $arrOrderDetail['realTicketFromTime'] = $v['TicketInfo']['TicketItem'][0]['RealFromTime'];\n\n            $arrOrderDetail['orderBusinessType'] = $v['OrderBusinessType'];\n\n            // 判断是否为流水班次\n            if ($v['TicketInfo']['TicketItem'][0]['ShiftType'] == 1) {\n                $arrOrderDetail['ticketFromTime'] = $arrOrderDetail['ticketFromTime'].'前';\n            }\n            $arrOrderDetail['ticketToTime'] = !empty($v['TicketInfo']['TicketItem'][0]['ToTime']) ? $v['TicketInfo']['TicketItem'][0]['ToTime'] : '--:--';\n            $arrOrderDetail['ticketToDays'] = !empty($v['TicketInfo']['TicketItem'][0]['ToDay']) ? intval($v['TicketInfo']['TicketItem'][0]['ToDay']) : 0;\n            $arrOrderDetail['totalFee'] = $v['TotalFee']*1;\n\n            $arrOrderDetail['canPay'] = 0;\n            $arrOrderDetail['canCancel'] = 0;\n\n            if (is_array($v['OrderOperate']) && !empty($v['OrderOperate'])) {\n                if (!isset($v['OrderOperate'][0])) {\n                    $v['OrderOperate'] = array($v['OrderOperate']);\n                }\n                foreach ($v['OrderOperate'] as $k1 => $v1) {\n                    if ($v1['operate'] == 'pay') {\n                        $arrOrderDetail['canPay'] = 1;\n                    }\n                    if ($v1['operate'] == 'cancelOrder') {\n                        $arrOrderDetail['canCancel'] = 1;\n                    }\n                }\n            }\n\n            // 判断是否铁友的订单,如果是铁友的订单才需要获取支付id\n            if ($arrData['ref'] != 'ctrip.h5') {\n                // 判断订单是否可以支付,如果可以支付则获取支付id\n                if ($arrOrderDetail['canPay']) {\n                    //$v['OrderId'] = $v['OrderId'];\n                    $arrPayInfo = $this->addOrderPayment($v);\n                    $arrOrderDetail['payId'] = intval($arrPayInfo['InsertID']);\n                } else {\n                    $arrOrderDetail['payId'] = 0;\n                }\n            }\n\n\n            $arrOrderDetail['lastPayDateTime'] = date('Y-m-d H:i:s', strtotime($v['OrderTime'].' +1 hour'));\n            require_once('bus/control/config/class.servicePackage.php');\n            $objServicePackage = new servicePackage();\n            $arrData['order_time'] = $v['OrderTime'];\n            $arrData['service_fee'] = intval($v['ServiceFee'] / $v['TicketCnt']);\n            $arrServicePackageConfig = $objServicePackage->getServicePackage($arrData);\n            // 判断是否需要显示支付金额明细\n            if ($v['TicketInfo']['TicketItem'][0]['ServiceFee'] > 0) {\n                $arrOrderDetail['totalFeeTitle'] = '(含'.intval($v['TicketInfo']['TicketItem'][0]['ServiceFee']).'元/人'.$v['DisplayServiceName'].')';\n            } else {\n                $arrOrderDetail['totalFeeTitle'] = '';\n            }\n            $arrOrderDetail['servicePhoneTitle'] = '联系客服：021-22268888';\n            $arrOrderDetail['servicePhone'] = '021-22268888';\n            $arrOrderDetail['fetcherNote'] = '取票说明';\n\n            // 拼装取票信息\n            $arrOrderDetail['fetcherInfo'] = array();\n            $arrOrderDetail['fetcherInfo'][] = array('k' => '取票人', 'v' => $v['FetcherName'], 's' => '');\n            $arrOrderDetail['fetcherInfo'][] = array('k' => '联系电话', 'v' => $this->maskString($v['UserInfo']['ContactMobile']), 's' => '');\n            $arrOrderDetail['fetcherInfo'][] = array('k' => strtr($v['FetcherPaper'], array('二代身份证' => '身份证')), 'v' => $this->maskString($v['DisplayFetcherPaperNum']), 's' => '');\n\n\n            // 获取车站信息\n            require_once('bus/control/api/class.busDataApi.php');\n            $objBusDataApi = new busDataApi();\n            $stationInfo = $objBusDataApi->getStationInfo($arrOrderDetail['fromStationName']);\n\n\n\n\n\n            // 判断是否购票成功\n            $arrOrderDetail['fetcherQrCodeInfo'] = array();\n            if ($v['ProcessState'] == 5) {\n                if (!empty($v['FetcherWebsiteOrderNumber'])) {\n                    $arrOrderDetail['fetcherInfo'][] = array('k' => '取票订单号', 'v' => $v['FetcherWebsiteOrderNumber'], 's' => '');\n                }\n                if (!empty($v['FetcherCode'])) {\n                    $arrOrderDetail['fetcherInfo'][] = array('k' => '取票号', 'v' => $v['FetcherCode'], 's' => '');\n                }\n                if (!empty($v['FetcherPassword'])) {\n                    $arrOrderDetail['fetcherInfo'][] = array('k' => '取票密码', 'v' => $v['FetcherPassword'], 's' => '');\n                }\n                if (!empty($v['FetcherVeryCode'])) {\n                    $arrOrderDetail['fetcherInfo'][] = array('k' => '取票验证码', 'v' => $v['FetcherVeryCode'], 's' => '');\n                }\n\n\n                // 默认车站经纬度坐标\n                $arrOrderDetail['coordinateX'] = '';\n                $arrOrderDetail['coordinateY'] = '';\n                // 需要显示车站经纬度坐标的字段节点\n                $arrOrderDetail['displayStationcoordinateNode'] = '';\n\n                if (is_array($stationInfo) && !empty($stationInfo)) {\n                    $arrOrderDetail['fetcherInfo'][] = array('k' => '取票车站', 'v' => $arrOrderDetail['fromStationName'], 's' => '');\n                    $arrOrderDetail['fetcherInfo'][] = array('k' => '车站地址', 'v' => $stationInfo['address'], 's' => '');\n                    $arrOrderDetail['fetcherInfo'][] = array('k' => '车站电话', 'v' => $stationInfo['phone_number'], 's' => '');\n\n                    //2016-03-02 废弃 start app 6.13.4\n                    $arrOrderDetail['coordinateX'] = empty($stationInfo['coordinate_x']) ? '' : $stationInfo['coordinate_x'];\n                    $arrOrderDetail['coordinateY'] = empty($stationInfo['coordinate_y']) ? '' : $stationInfo['coordinate_y'];\n                    //2016-03-02 废弃 end\n                }\n\n                // 获取取票二维码\n                if (!empty($v['FetcherQrCodeInfo']['Item'])) {\n                    if (!isset($v['FetcherQrCodeInfo']['Item'][0])) {\n                        $arrOrderDetail['fetcherQrCodeInfo'] = array($v['FetcherQrCodeInfo']['Item']);\n                    } else {\n                        $arrOrderDetail['fetcherQrCodeInfo'] = $v['FetcherQrCodeInfo']['Item'];\n                    }\n                }\n            }\n\n            /*k:取票人 v:高莎莎 s:\n            k:联系电话 v:13512345678 s:\n            k:车站电话 v:021-24099999 s:tel*/\n\n            // 拼装乘客信息\n            $arrTicketInfo = array();\n            $arrTicketTypeDesc = array(\n                '1' => '成人票',\n                '2' => '儿童票',\n                '3' => '携童票'\n            );\n\n            $refundParam = array('user_id'=>$response['Order']['UserInfo']['UserID'],'order_number'=>$arrOrderDetail['orderNumber']);\n            $canRefund = $this->_objOrderApi->isCanRefundTicket($refundParam);\n            $refundTicketList = array();\n            if($canRefund['code']==1){\n                if($canRefund['return']['refund_ticket_type']==1 and isset($canRefund['return']['can_refund_ticket_item'])){\n                    $refundTicketList = $canRefund['return']['can_refund_ticket_item'];\n                    $arrOrderDetail['refundTicketType'] = 1;\n                }else{\n                    $arrOrderDetail['refundTicketType'] = 2;\n                }\n\n            }else{\n                if($canRefund['stationCanRefund']==1){\n                    $arrOrderDetail['refundTicketType'] = 0;\n                    $arrOrderDetail['stationCanRefund'] = 1; //车站支持退票\n                    $arrOrderDetail['refundErrorMessage'] = $canRefund['message']; // 无法退票原因\n                }else{\n                    $arrOrderDetail['refundTicketType'] = 0;\n                }\n            }\n\n\n            $arrSeatNumber = array();\n            foreach ($v['TicketInfo']['TicketItem'] as $k1 => $v1) {\n\n                $ticketInfo['ticketId'] = $v1['TicketId'];\n                $ticketInfo['identityName'] = $v1['IdentityName'];\n                $ticketInfo['identityType'] = strtr($v1['IdentityType'], array('二代身份证' => '身份证'));\n                $ticketInfo['identityCode'] = $this->maskString($v1['DisplayIdentityCode']);\n                $ticketInfo['ticketPrice'] = $v1['TicketPrice']*1;\n                $ticketInfo['ticketType'] = $arrTicketTypeDesc[$v1['TicketType']];\n                $ticketInfo['canRefund'] = in_array($ticketInfo['ticketId'],$refundTicketList) ? 1 : 0;\n                $ticketInfo['refudnState'] = intval($v1['ReturnState']);\n                //儿童出生日期\n                if($v1['BirthDate']){\n                    $ticketInfo['birthDate'] = $v1['BirthDate'];\n                }\n\n                switch ($v1['ReturnState']) {\n                    case '1':\n                        $ticketInfo['refudnDesc'] = '待处理';\n                        break;\n                    case '2':\n                        $ticketInfo['refudnDesc'] = '退票成功';\n                        break;\n                    case '3':\n                        $ticketInfo['refudnDesc'] = '退票失败';\n                        break;\n\n                    default:\n                        $ticketInfo['refudnDesc'] = '';\n                        break;\n                }\n\n                $arrTicketInfo[] = $ticketInfo;\n                if (!empty($v1['SeatNumber'])) {\n                    $arrSeatNumber[] = $v1['SeatNumber'];\n                }\n            }\n\n            $arrOrderDetail['ticketInfo'] = $arrTicketInfo;\n\n            $info = $this->_objOrderApi->orderRefundInfo($arrOrderDetail['orderNumber']);\n            $arrOrderDetail['isShowRefund']  = $info['code']==1 ? true : false;\n            $refundFeeBill = array();\n            if($arrOrderDetail['isShowRefund']){\n                foreach ($info['return'] as $key => $value) {\n                    array_push($refundFeeBill,$value['bill_no']);\n                }\n                $arrOrderDetail['refundFeeBill'] = implode(',', $refundFeeBill);\n                unset($refundFeeBill);\n            }\n\n\n            $arrOrderDetail['messageBox'] = array();\n\n\n            if($userInfo['Gender']==2){\n                $arrOrderDetail['messageBox']['headImgUrl'] = 'http://pic.c-ctrip.com/h5/train/qp-avs/m'.rand(1,11).'m.jpg';\n                $arrOrderDetail['messageBox']['csName'] = '鲜肉客服';\n            }else if($userInfo['Gender']==1){\n                $arrOrderDetail['messageBox']['headImgUrl'] = 'http://pic.c-ctrip.com/h5/train/qp-avs/f'.rand(1,14).'m.jpg';\n                $arrOrderDetail['messageBox']['csName'] = '美女客服';\n            }else{\n                $genderArray = array('m','f');\n                $gkey = array_rand($genderArray,1);\n                $csImg =  ($genderArray[$gkey] == 'm') ? 'm'.rand(1,11) : 'f'.rand(1,14);\n                $arrOrderDetail['messageBox']['csName'] = $genderArray[$gkey] == 'm' ? '鲜肉客服' : '美女客服';\n                $arrOrderDetail['messageBox']['headImgUrl'] = 'http://pic.c-ctrip.com/h5/train/qp-avs/'.$csImg.'m.jpg';\n            }\n\n            $arrOrderDetail['messageBox']['msg'] = array();\n            // 出发车站地址\n            $stationAddress = '';\n            $stationAddress = !empty($stationInfo['address']) ? '：'.$stationInfo['address'] : '';\n            // 取票方式\n            $fetcherTicketWay = '';\n            $fetcherTicketWay = !empty($stationInfo['fetcher_identity_type']) ? str_replace('|','或',$stationInfo['fetcher_identity_type']) : '取票码或身份证' ;\n\n\n            switch ($arrOrderDetail['orderState']) {\n                case '待支付':\n                    $arrOrderDetail['messageBox']['msg']='请在'.date('H:i',strtotime($v['OrderTime'])+3600).'前完成支付哦~';\n                    break;\n                case '支付失败':\n                    $arrOrderDetail['messageBox']['msg']='抱歉，支付失败！可尝试重新支付哦`';\n                    break;\n                case '超时未支付':\n                    $arrOrderDetail['messageBox']['msg']='抱歉，您的订单长时间未支付，已经帮您取消啦~';\n                    break;\n                case '支付处理中':\n                    $arrOrderDetail['messageBox']['msg']='正在帮您火速处理，请稍等片刻！';\n                    break;\n                case '等待购票':\n                    $arrOrderDetail['messageBox']['msg']='我们会在第一时间帮你出票，稍等片刻哦~';\n                    break;\n                case '购票中':\n                    $arrOrderDetail['messageBox']['msg']='正在帮您购票哦，请稍等片刻，车票马上就来啦！';\n                    break;\n                case '已成交':\n                    $arrOrderDetail['messageBox']['msg']='请凭'.$fetcherTicketWay.'去 <span>['.$arrOrderDetail['fromStationName'].$stationAddress.']</span> 取票';\n                    break;\n                case '退款中':\n                    $arrOrderDetail['messageBox']['msg']='抱歉，购票失败，正在为您退款，预计1-7个工作日退回原账户~';\n                    break;\n                case '全部退款':\n                    $arrOrderDetail['messageBox']['msg']='抱歉，购票失败，已为您全额退款，预计1-7个工作日退回原账户~';\n                    break;\n                case '已取消':\n                    $arrOrderDetail['messageBox']['msg']='期待您的下次光临，么么哒~';\n                    break;\n                default:\n                    # code...\n                    break;\n            }\n\n            $refundTicket = $this->_objOrderApi->orderRefundInfoTicket($arrOrderDetail['orderNumber']);\n            $refudnTiketTemp = array();\n            if(!empty($refundTicket['return'])){\n                foreach ($refundTicket['return'] as $key => $value) {\n                    if($value['refund_fee_state'] == 2){\n                        array_push($refudnTiketTemp,$value['ticket_user_name'].'已退票');\n                    }\n                }\n            }\n            !empty($refudnTiketTemp)?$arrOrderDetail['messageBox']['refundTicketMsg'] = array_pop($refudnTiketTemp) : null ;\n\n\n\n            // 新增支付扩展节点\n            $arrOrderDetail['payExtendInfo'] = $v['PayExtendInfo'];\n            // 新增实际支付金额字段（跳转到支付平台的支付金额以这个字段返回的金额为准）\n            $arrOrderDetail['displayRealPayFee'] = $v['DisplayRealPayFee']*1;\n            /*orderNumber      订单号\n            orderState       车票状态\n            orderShowStyle   订单显示样式 1 正常 2 灰 3 高亮\n            orderDateTime    下单时间\n            fromCityName     出发城市（示例：北京）\n            toCityName       到达城市（示例：上海）\n            fromStationName  出发站名（示例：赵公口客运站）\n            toStationName    到达站名（示例：）\n            fromShowName     出发显示名\n            toShowName       到达显示名\n            ticketDate       出发时期（示例：2014-05-01）\n            ticketFromTime   出发时间（示例：10:00）\n            ticketToTime     到达时间（示例：10:00）\n            totalFee         订单价格 1800\n\n            canPay           可以支付 1 可以 0 不可以\n            canCancel        可以取消 1 可以 0 不可以\n            lastPayDateTime  最后支付时间\n            totalFeeTitle    费用显示 订单总额（含服务费 5元/人）:\n            totalFee         订单价格\n            servicePhoneTitle 联系客服：021-24099999\n            servicePhone      021-24099999\n\n            fetcherNote      取票说明\n            fetcherInfo      取票信息 数组\n            k:取票人 v:高莎莎 s:\n            k:联系电话 v:13512345678 s:\n            k:车站电话 v:021-24099999 s:tel\n            ticketInfo 乘客信息  数组\n            ….*/\n            $arrOrderDetail['cashBack'] = intval($v['VendorInfo']['ReturnPrice']);\n            $arrOrderDetail['cashBackDesc'] = '' ;\n\n            if($arrOrderDetail['cashBack'] > 0){\n                if($v['VendorInfo']['ReturnType']=='1'){\n                    $arrOrderDetail['cashBackDesc'] = '下单并成功出行后3个工作日内，可获返'.$arrOrderDetail['cashBack'].'元任我游礼品卡至礼品卡账户';\n                }else if($v['VendorInfo']['ReturnType']=='2'){\n                    $arrOrderDetail['cashBackDesc'] = '下单并成功出行后3个工作日内，可获返'.$arrOrderDetail['cashBack'].'元任我行礼品卡至礼品卡账户';\n                }else if($v['VendorInfo']['ReturnType']=='3'){\n                    $arrOrderDetail['cashBackDesc'] = '下单并成功出行后3个工作日内，可获返'.$arrOrderDetail['cashBack'].'元现金至支付账户';\n                }else{\n                    $arrOrderDetail['cashBackDesc'] = '';\n                }\n            }\n\n\n\n\n            // 获取用户的支付方式\n            if (!isset($v['UserPayMethod']['Item'][0])) {\n                $v['UserPayMethod']['Item'] = array($v['UserPayMethod']['Item']);\n            }\n            $arrOrderDetail['userPayMethod'] = !empty($v['UserPayMethod']['Item']) ? $v['UserPayMethod']['Item'] : array();\n\n            // 获取价格明细\n            if (!isset($v['PriceList']['Item'][0])) {\n                $v['PriceList']['Item'] = array($v['PriceList']['Item']);\n            }\n\n            $arrOrderDetail['priceList'] = !empty($v['PriceList']['Item']) ? $v['PriceList']['Item'] : array();\n\n            $busInfo = $this->_objBusDataApi->getBus($arrOrderDetail['fromCityName'], $arrOrderDetail['toCityName'], $arrOrderDetail['fromStationName'], $arrOrderDetail['busNumber'], $arrOrderDetail['ticketDate'], $v['TicketInfo']['TicketItem'][0]['TicketTime'],$arrOrderDetail['toStationName']);\n            $arrOrderDetail['utmSource'] = $v['UtmSource'];\n            $simplifyConfig = $this->_objBusDataApi->getSimplifyConfig($busInfo,$arrOrderDetail['utmSource']);\n            if($simplifyConfig){\n                $arrOrderDetail['simplifyConfig'] = $simplifyConfig;\n            }\n\n\n            //取票人新接口\n            $arrOrderDetail['contactInfo']['name'] = $v['FetcherName'];\n            $arrOrderDetail['contactInfo']['mobile'] = $this->maskString($v['UserInfo']['ContactMobile']);\n            $arrOrderDetail['contactInfo']['cardType'] = strtr($v['FetcherPaper'], array('二代身份证' => '身份证'));\n            $arrOrderDetail['contactInfo']['cardNum'] = $this->maskString($v['DisplayFetcherPaperNum']);\n            //车站信息新接口\n\n            $arrOrderDetail['displayStationcoordinateNode'] = '';\n\n\n            //车站信息\n            if (is_array($stationInfo) && !empty($stationInfo)) {\n                $arrOrderDetail['stationInfo']['name'] =$arrOrderDetail['fromStationName'];\n                $arrOrderDetail['stationInfo']['address'] = $stationInfo['address'];\n                $arrOrderDetail['stationInfo']['tel'] = $stationInfo['phone_number'];\n\n                $arrOrderDetail['stationInfo']['coordinateX'] = empty($stationInfo['coordinate_x']) ? '' : $stationInfo['coordinate_x'];\n                $arrOrderDetail['stationInfo']['coordinateY'] = empty($stationInfo['coordinate_y']) ? '' : $stationInfo['coordinate_y'];\n                // 判断车站经纬度坐标是否为空\n                if (!empty($arrOrderDetail['coordinateX']) && !empty($arrOrderDetail['coordinateY'])) {\n                    $arrOrderDetail['displayStationcoordinateNode'] = '车站地址';\n                }\n                $baiduCoords = $objBusDataApi->getBaiduCoords( $arrOrderDetail['stationInfo']['coordinateX'],$arrOrderDetail['stationInfo']['coordinateY']);\n                $arrOrderDetail['stationInfo']['baiduX'] = $baiduCoords['x'];\n                $arrOrderDetail['stationInfo']['baiduY'] = $baiduCoords['y'];\n\n            }\n\n            // 迪斯尼退改文案\n            require_once('bus/control/config/class.backMealCopyConfig.php');\n            $backMealCopyConfig = new backMealCopyConfig();\n            if ($backMeakCopyConfig = backMealCopyConfig::getBackMealCopyConfig($arrOrderDetail['fromCityName'].\"-\".$arrOrderDetail['toCityName'])) {\n\n                $arrOrderDetail = array_merge($arrOrderDetail, $backMeakCopyConfig);\n\n            } else {\n                // 获取服务说明文案\n                require_once('bus/control/config/class.lineAndDescriptionConfig.php');\n                $lineAndDesConfig = new lineAndDescriptionConfig();\n                $stationCanRefundTicket = $busInfo ? $busInfo['from_station_info']['ticket_return'] : 0 ;\n                $arrOrderDetail['serviceDescNew'] = $lineAndDesConfig->getBusDescriptionNew(array('bus_number'=>$arrOrderDetail['busNumber'],'activity_offsettype' => $v['ActivityOffsetType'],'canRefundTicket'=>$stationInfo['ticket_return']));\n            }\n\n\n            //取票信息\n            if (!empty($v['FetcherWebsiteOrderNumber'])) {\n                $arrOrderDetail['takeInfoOther']['takeOrderNum'] = str_replace(',',' , ',$v['FetcherWebsiteOrderNumber']);\n            }\n            if (!empty($v['FetcherCode'])) {\n                $arrOrderDetail['takeInfoOther']['takeNum'] = $v['FetcherCode'];\n            }\n            if(!empty($v['FetcherPassword'])){\n                $arrOrderDetail['takeInfoOther']['takePass'] = $v['FetcherPassword'];\n            }\n            if (!empty($v['FetcherVeryCode'])) {\n                $arrOrderDetail['takeInfoOther']['takeCode'] = $v['FetcherVeryCode'];\n            }\n            // 检票口\n            if (!empty($v['TicketInfo']['TicketItem'][0]['TicketCheck'])) {\n                $arrOrderDetail['takeInfoOther']['ticketCheck'] = $v['TicketInfo']['TicketItem'][0]['TicketCheck'];\n            }\n            // 座位号\n            if (!empty($arrSeatNumber)) {\n                $arrOrderDetail['takeInfoOther']['seatNumber'] = implode(',', $arrSeatNumber);\n            }\n\n            //配送票信息\n            if($v['DispatchType']>0){\n                $arrOrderDetail['DispatchInfo']['Receiver'] = $v['DispatchInfo']['Receiver'];\n                $arrOrderDetail['DispatchInfo']['ReceiverMobile'] = $this->maskString($v['DispatchInfo']['ReceiverMobile']);\n                $arrOrderDetail['DispatchInfo']['DisplayDispatchTypeFee'] = $v['DisplayDispatchTypeFee'];\n                $arrOrderDetail['DispatchInfo']['DisplayDispatchReceiverAddress'] = $v['DisplayDispatchReceiverAddress'];\n                $arrOrderDetail['DispatchInfo']['DisplayDispatchExpressCompany'] = $v['DisplayDispatchExpressCompany'];\n            }\n            //配送票信息\n            if(!empty($v['Disney'])){\n                $strDisney = '';\n                $strDisneyMajor = '';\n                $strDisneyChild = '';\n                $strDisneyState = '';\n                //'上海迪士尼乐园门票（2016-06-21，成人票2张，儿童票1张）出票成功啦！请凭洪芮、李金泽的身份证入园哦~';\n                if($v['Disney']['DisneyMajorCnt']>0){\n                    $strDisneyMajor = '成人票'.$v['Disney']['DisneyMajorCnt'].'张';\n                }\n                if($v['Disney']['DisneyChildCnt']>0){\n                    if($v['Disney']['DisneyMajorCnt']>0){\n                        $strDisneyChild = '，儿童票'.$v['Disney']['DisneyChildCnt'].'张';\n                    }else{\n                        $strDisneyChild = '儿童票'.$v['Disney']['DisneyChildCnt'].'张';\n                    }\n                }\n                switch($v['Disney']['DisneyState']){\n                    case 0:\n                        if ($v['ProcessState'] < 3) {\n                            $strDisneyState = '待支付~';\n                        }else{\n                            $strDisneyState = '正在出票中~';\n                        }\n                        break;\n                    case 1:\n                        $strDisneyState = '等待支付中~';\n                        break;\n                    case 2:\n                        $strDisneyState = '正在出票中~';\n                        break;\n                    case 3:\n                        $strDisneyState = '出票成功啦！请凭'.$v['FetcherName'].'的身份证入园哦~';\n                        break;\n                    case 4:\n                        $strDisneyState = '出票失败~';\n                        break;\n                    case 5:\n                        $strDisneyState = '退款中~';\n                        break;\n                    case 6:\n                        $strDisneyState = '退款成功~';\n                        break;\n                    default:\n                        break;\n                }\n                $strDisney = '上海迪士尼乐园门票（'.$v['Disney']['DisneyDttm'].'，'.$strDisneyMajor.$strDisneyChild.')'.$strDisneyState;\n                $arrOrderDetail['Disney']['Content'] = $strDisney;\n            }\n\n            //抢票任务信息\n            if($v['IsGrabTicket']>0){\n                $fromStationName = str_replace(\"|\",\"、\",$v['GrabTicketInfo']['FromStationName']);\n                $arrOrderDetail['GrabTicketInfo'][] = array('k' => '出发车站', 'v' => $fromStationName);\n                $arrOrderDetail['GrabTicketInfo'][] = array('k' => '出发日期', 'v' => $v['GrabTicketInfo']['TicketDate'].' '.$v['GrabTicketInfo']['TicketTime']);\n                $startBookingDttm = date(\"m月d日\",strtotime($v['StartBookingDttm']));\n                $processState = '';\n                $processStateMsg = '';\n                switch($v['Status']){\n                    case \"等待购票\":\n                        $processState = 5;\n                        $processStateMsg = \"未到预售期，预计{$startBookingDttm}开抢\";\n                        break;\n                    case \"购票中\":\n                        $processState = 6;\n                        $processStateMsg = \"进入预售期，拼命抢票中...\";\n                        break;\n                    case \"已成交\":\n                        $processState = 7;\n                        $processStateMsg = \"抢票成功啦！\";\n                        break;\n                    case \"退款中\":\n                        $processState = 8;\n                        $processStateMsg = \"伤心！抢票失败！\";\n                        break;\n                    case \"全部退款\":\n                        $processState = 9;\n                        $processStateMsg = \"伤心！抢票失败！\";\n                        break;\n                    default:\n                        break;\n                }\n                if($processState){\n                    $arrOrderDetail['GrabTicketInfoState'] = array('k' => $processState, 'v' => $processStateMsg);\n                }\n            }\n\n            //新增预售日期字段 支付成功处理中\n            if(($v['ProcessState'] == 3 ||$v['ProcessState'] == 4) && strtotime(date('Y-m-d H:i:s')) < strtotime($v['StartBookingDttm'])){\n                $arrOrderDetail['openBookDate'] = $v['StartBookingDttm'];\n                $arrOrderDetail['optionalBusInfo'] = \"预约票预计{$v['StartBookingDttm']}为您出票，高峰期发车时间可能会有变动，请以短信为准。\";\n            }elseif($this->setOrderTicketChangeTip()){\n                //订单详情出票或抢票状态免责提示(备用)\n                $arrOrderDetail['optionalBusInfo']='';\n            }\n\n            //打包产品\n            $piao  = Xconfig::getOrderPiao($v['OrderNumber']);\n            $coupon = XConfig::getOrderCoupon($v['OrderNumber']);\n            if($piao && $v['PackageFlag'] != 2){\n                $productPackage = array();\n                $productPackage['activity_title'] = $piao['product_name'];\n                $productPackage['activity_img'] = $piao['product_img'];\n                $productPackage['activity_desc_title'] = $piao['product_desc']['activity_desc_title'];\n                $productPackage['activity_desc_bus_explain'] = $piao['product_desc']['activity_desc_bus_explain'];\n                $productPackage['activity_desc_explain'] = $piao['product_desc']['activity_desc_explain'];\n                $productPackage['activity_desc_bus_use_rules'] = $piao['product_desc']['use_rules'];\n                $productPackage['ticket_fee'] = $v['TicketInfo']['TicketItem'][0]['TicketPrice'];\n                $productPackage['activity_desc_native_price'] =  $piao['product_original_price'];\n                $productPackage['productType'] = 'piao';\n                $productPackage['packagePrice'] = $productPackage['ticket_fee'] + $piao['product_desc']['activity_desc_sell_price'];\n                $productPackage['oldPackagePrice'] = $productPackage['ticket_fee'] + $piao['product_desc']['activity_desc_native_price'];\n\n                $productPackage['subtitle'] = '内含1张汽车票+1张'.$productPackage['activity_title'];\n\n                $arrOrderDetail['productPackage'] = $productPackage;\n\n\n\n\n                if(isset($arrOrderDetail['priceList'][0])){\n                    $arrOrderDetail['priceList'][0] = array(\n                        'name'=>'套餐价',\n                        'desc'=>'￥'.($piao['product_desc']['activity_desc_sell_price']+$v['TicketInfo']['TicketItem'][0]['TicketPrice']).' x '.$v['MajorCnt'].'份',\n                    );\n                }\n\n            }\n            if($coupon){\n                $arrOrderDetail['productPackage'] = $coupon;\n                $arrOrderDetail['productPackage']['productType'] = 'coupon';\n\n\n                //区分优惠劵类型\n                if(in_array($arrOrderDetail['productPackage']['couponStrategyID'],array('28482','26023'))){\n                    $couponTypeName = '酒店';\n                    $arrOrderDetail['productPackage']['x_PS'] = '车票酒店，齐购享优惠';\n                }\n                else if(in_array($arrOrderDetail['productPackage']['couponStrategyID'],array('25554'))){\n                    $couponTypeName = '门票玩乐';\n                    $arrOrderDetail['productPackage']['x_PS'] = '出游套餐，齐购享优惠';\n                }\n                else{\n                    $couponTypeName = '';\n                    $arrOrderDetail['productPackage']['x_PS'] = '出游套餐，齐购享优惠';\n                }\n\n                //差价\n                if(in_array($arrOrderDetail['productPackage']['couponStrategyID'],array('28482','25554')) and $arrOrderDetail['ticketInfo'][0]['ticketPrice']>5){\n                    $arrOrderDetail['productPackage']['diffPrice'] = 5;\n\n                }else{\n                    $arrOrderDetail['productPackage']['diffPrice'] = 0;\n                }\n\n                $arrOrderDetail['productPackage']['x_price'] = $arrOrderDetail['productPackage']['price']+$arrOrderDetail['productPackage']['diffPrice'];\n                $arrOrderDetail['productPackage']['x_ticketPrice'] = $arrOrderDetail['ticketInfo'][0]['ticketPrice']-$arrOrderDetail['productPackage']['diffPrice'];\n\n                if ($arrOrderDetail['productPackage']['value']>0) {\n                    $arrOrderDetail['productPackage']['x_subtitle'] = '内含汽车票+￥'.$arrOrderDetail['productPackage']['x_price'].$couponTypeName.'优惠劵（抵'.$arrOrderDetail['productPackage']['value'].'元）';            \n                } else {\n                    $arrOrderDetail['productPackage']['x_subtitle'] = '内含汽车票+￥'.$arrOrderDetail['productPackage']['x_price'].$couponTypeName.'优惠劵（全年仅一次，6折包车）'; \n                }\n\n\n\n                $arrOrderDetail['productPackage']['subtitle'] = '内含1张汽车票+'.$arrOrderDetail['productPackage']['num'].'张'.$arrOrderDetail['productPackage']['title'];\n                $arrOrderDetail['productPackage']['packagePrice'] = $arrOrderDetail['ticketInfo'][0]['ticketPrice']+$arrOrderDetail['productPackage']['price'];\n                $arrOrderDetail['productPackage']['oldPackagePrice'] = $arrOrderDetail['ticketInfo'][0]['ticketPrice'] + $arrOrderDetail['productPackage']['value'];\n\n\n                if(isset($arrOrderDetail['priceList'][0])){\n                    $arrOrderDetail['priceList'][0] = array(\n                        'name'=>'套餐价',\n                        'desc'=>'￥'.($coupon['price']+$v['TicketInfo']['TicketItem'][0]['TicketPrice']).' x '.$v['MajorCnt'].'份',\n                    );\n                }\n\n                // 当套餐原价为0 显示-\n                if ($coupon['value'] == 0) {\n                    $arrOrderDetail['productPackage']['value'] = \"-\";\n                }\n                if ($coupon['oldPrice'] == 0) {\n                    $arrOrderDetail['productPackage']['oldPrice'] = \"-\";\n                }\n            }else{\n                // 判断填写页酒店优惠券\n                $hotelCoupon = XConfig::getOrderHotelCoupon($v['OrderNumber']);\n                if($hotelCoupon){\n                    $hotelCouponPrice = !empty($hotelCoupon['coupon_price']) ? $hotelCoupon['coupon_price'] : $hotelCoupon[0]['coupon_price'];\n                    $arrHotelCouponDetail = array(\n                        'name'=>'酒店优惠券',\n                        'desc'=>'￥'.$hotelCouponPrice.' x '.$v['MajorCnt'].'份',\n                    );\n                    array_push($arrOrderDetail['priceList'],$arrHotelCouponDetail);\n                }\n            }\n\n            //支付详情 start\n\n            $osTickInfo = array();\n            foreach ($arrTicketInfo as $key => $value) {\n                array_push($osTickInfo,array(\n                    'KOrderDetailMinorTitle'=>$value['identityName'],\n                    'KOrderDetailMinorComment'=>strtr($value['identityType'], array('身份证' => '二代身份证')).'：'.$value['identityCode']\n                ));\n            }\n\n            if($v['IsGrabTicket']>0){\n                $payFromTimeString = $arrOrderDetail['ticketDate'].' '.$v['GrabTicketInfo']['TicketTime'];\n            }else{\n                $payFromTimeString = $arrOrderDetail['ticketDate'].' '.$arrOrderDetail['ticketFromTime'];\n            }\n\n            $orderSummary = array(\n                'KOrderSummary'=>array(\n                    'KOrderMainTitle'=>'单程 '.$arrOrderDetail['fromStationName'].' - '.$arrOrderDetail['toCityName'],\n                    'KOrderMinorTitle'=>'出发：'.$payFromTimeString.' 汽车票余票变动频繁，请尽量在15分钟内完成支付，可提高出票成功率哦~',\n                ),\n                'KOrderDetail'=>array(\n                    array(\n                        'KOrderDetailMainTitle'=>'乘车人',\n                        'KOrderDetailMinor'=>$osTickInfo,\n                    )\n                )\n            );\n\n            if($v['DispatchType'] > 0){\n\n                array_push($orderSummary['KOrderDetail'],array(\n                    'KOrderDetailMainTitle'=>'配送信息',\n                    'KOrderDetailMainValue'=>$arrOrderDetail['DispatchInfo']['Receiver'],\n                    'KOrderDetailMainComment'=> $arrOrderDetail['DispatchInfo']['DisplayDispatchReceiverAddress'],\n                ));\n                array_push($orderSummary['KOrderDetail'],array(\n                    'KOrderDetailMainTitle'=> '联系电话',\n                    'KOrderDetailMainValue'=> $arrOrderDetail['DispatchInfo']['ReceiverMobile'],\n                ));\n\n            }else{\n                array_push($orderSummary['KOrderDetail'],array(\n                    'KOrderDetailMainTitle'=> '取票人',\n                    'KOrderDetailMainValue'=> $arrOrderDetail['contactInfo']['name'],\n                    'KOrderDetailMainComment'=> $arrOrderDetail['contactInfo']['cardType'].'：'.$arrOrderDetail['contactInfo']['cardNum']\n                ));\n                array_push($orderSummary['KOrderDetail'],array(\n                    'KOrderDetailMainTitle'=> '联系电话',\n                    'KOrderDetailMainValue'=> $arrOrderDetail['contactInfo']['mobile'],\n                ));\n            }\n            $arrOrderDetail['orderSummary'] = json_encode($orderSummary);\n            //支付详情 end\n\n\n\n\n            $arrOrderDetail['freeInsurance'] = $v['FreeInsurance'];\n            if(isset($v['PostInvoice'])){\n                $arrOrderDetail['invoiceInfo'] = $v['PostInvoice'];\n            }\n            require_once('bus/control/config/class.lineAndDescriptionConfig.php');\n            $lineAndDesConfig = new lineAndDescriptionConfig();\n            $newCar=$lineAndDesConfig->changeCarType($arrOrderDetail['fromCityName'].\"-\".$arrOrderDetail['toCityName'].\"-\".$arrOrderDetail['busType']);\n            if(!empty($newCar)){\n                $arrOrderDetail['newCar']=$newCar;\n            }\n\n            //携童数\n            if($v['TakeChildCnt']){\n                $arrOrderDetail['takeChildCnt'] = $v['TakeChildCnt'];\n            }\n            // 获取1元免单\n            if(!empty($v['salePackageInfo'])){\n                $arrOneFreeOrders = array('name'=>$v['salePackageInfo']['name'],'unit'=>$v['salePackageInfo']['unit'],'url'=>$v['salePackageInfo']['url']);\n                $arrOrderDetail['salePackageInfo'][] = $arrOneFreeOrders;\n            }\n\n            $arrReturn['code'] = 1;\n            $arrReturn['message'] = '';\n            $arrReturn['return'] = $arrOrderDetail;\n            return $arrReturn;\n        }else{\n            $arrReturn['code'] = 0;\n            $arrReturn['message'] = $response['ErrorResponse']['ErrorMessage'];\n            $arrReturn['return'] = '';\n            return $arrReturn;\n        }\n    }\n\n    /**\n     * 点到点和汽车票订单的混合订单详情\n     * 先去请求一下订单详情，如果是点到点的订单就走点到点的清单详情\n     * 否则走原来汽车票订单详情的接口\n     */\n    public function getOrderDetailAll($arrData){\n        $response = $this->_objOrderApi->orderDetail($arrData['orderNumber']);\n        if($response['Status']=='SUCCESS'){\n            if($response['Order']['BookType']==3){\n                $arrReturn=$this->pointOrderDetail($arrData,$response);\n            }else{\n                $arrReturn=$this->orderDetail($arrData,$response);\n            }\n            return $arrReturn;\n        }else{\n            $arrReturn['code'] = 0;\n            $arrReturn['message'] = $response['ErrorResponse']['ErrorMessage'];\n            $arrReturn['return'] = '1111';\n            return $arrReturn;\n        }\n    }\n    /*\n     * 点到点车次详情拼装\n     * */\n    private function pointOrderDetail($arrData,$response){\n        $arrLoginUserInfo = $this->checkUserApi($arrData);\n        if ($arrLoginUserInfo['code'] != 1) {\n            return $arrLoginUserInfo;\n        }\n        $objAuthApi = new authApi();\n        $userInfo = $objAuthApi->getUserInfoToH5(array('Auth'=>$arrData['head']['auth'],'BizType'=>'Other'));\n\n        // 判断是否是该用户的订单\n        if ($response['Order']['UserInfo']['UserID'] != $arrLoginUserInfo['return']['userID']) {\n            //接入公共防鉴权服务\n            $checkAuthResult = self::checkAuth($response['Order']);\n            //print_r($checkAuthResult);die;\n            if($checkAuthResult['ReturnCode'] != 0){\n                $arrReturn['code'] = 0;\n                $arrReturn['message'] = '';\n                $arrReturn['return'] = $checkAuthResult;\n                return $arrReturn;\n            }\n        }\n        $v = $response['Order'];\n        if (!isset($v['TicketInfo']['TicketItem'][0])) {\n            $v['TicketInfo']['TicketItem'] = array($v['TicketInfo']['TicketItem']);\n        }\n        $arrOrderDetail['orderNumber'] = $v['OrderNumber'];\n        $arrOrderDetail['orderState'] = $v['Status'];\n        $arrOrderDetail['TradeNo'] = $v['TradeNo'];\n        $arrOrderDetail['orderShowStyle'] = 1;\n        $arrOrderDetail['orderDateTime'] = substr($v['OrderTime'], 0, 10);\n        $arrOrderDetail['busNumber'] = $v['TicketInfo']['TicketItem'][0]['BusNumber'];\n        $arrOrderDetail['busType'] = !empty($v['TicketInfo']['TicketItem'][0]['BusType']) ? $v['TicketInfo']['TicketItem'][0]['BusType'] : '';\n        $arrOrderDetail['fromCityName'] = $v['TicketInfo']['TicketItem'][0]['FromCityName'];\n        $arrOrderDetail['toCityName'] = $v['TicketInfo']['TicketItem'][0]['ToCityName'];\n        $arrOrderDetail['fromStationName'] = $v['TicketInfo']['TicketItem'][0]['FromStationName'];\n        $arrOrderDetail['toStationName'] = $v['TicketInfo']['TicketItem'][0]['ToStationName'];\n        if (empty($arrOrderDetail['toStationName'])) {\n            $arrOrderDetail['toStationName'] = $arrOrderDetail['toCityName'];\n        }\n        $arrOrderDetail['fromShowName'] = $arrOrderDetail['fromCityName'];\n        $arrOrderDetail['toShowName'] = $arrOrderDetail['toCityName'];\n        $arrOrderDetail['ticketDate'] = $v['TicketInfo']['TicketItem'][0]['TicketDate'];\n        $arrOrderDetail['ticketFromTime'] = $v['TicketInfo']['TicketItem'][0]['TicketTime'];\n        $arrOrderDetail['ticketToTime'] = !empty($v['TicketInfo']['TicketItem'][0]['ToTime']) ? $v['TicketInfo']['TicketItem'][0]['ToTime'] : '--:--';\n        $arrOrderDetail['ticketToDays'] = !empty($v['TicketInfo']['TicketItem'][0]['ToDay']) ? intval($v['TicketInfo']['TicketItem'][0]['ToDay']) : 0;\n        $arrOrderDetail['totalFee'] = $v['TotalFee']*1;\n        //点到点上车点地址\n        $arrOrderDetail['stationDetailAddress']=$v['TicketInfo']['TicketItem'][0]['OnBoardAddress'];\n        //点到点司机手机号码，车型，车牌号\n        $arrOrderDetail['driverTel']=$v['DriverTel'];\n        $arrOrderDetail['busCreditCard']=$v['BusCreditCard'];\n\n        $arrOrderDetail['canPay'] = 0;\n        $arrOrderDetail['canCancel'] = 0;\n        if (is_array($v['OrderOperate']) && !empty($v['OrderOperate'])) {\n            if (!isset($v['OrderOperate'][0])) {\n                $v['OrderOperate'] = array($v['OrderOperate']);\n            }\n            foreach ($v['OrderOperate'] as $k1 => $v1) {\n                if ($v1['operate'] == 'pay') {\n                    $arrOrderDetail['canPay'] = 1;\n                }\n                if ($v1['operate'] == 'cancelOrder') {\n                    $arrOrderDetail['canCancel'] = 1;\n                }\n            }\n        }\n\n        if ($arrData['ref'] != 'ctrip.h5') {\n            // 判断订单是否可以支付,如果可以支付则获取支付id\n            if ($arrOrderDetail['canPay']) {\n                //$v['OrderId'] = $v['OrderId'];\n                $arrPayInfo = $this->addOrderPayment($v);\n                $arrOrderDetail['payId'] = intval($arrPayInfo['InsertID']);\n            } else {\n                $arrOrderDetail['payId'] = 0;\n            }\n        }\n        $arrOrderDetail['lastPayDateTime'] = date('Y-m-d H:i:s', strtotime($v['OrderTime'].' +1 hour'));\n        $arrOrderDetail['totalFeeTitle'] = '';\n        $arrOrderDetail['servicePhoneTitle'] = '联系客服：021-22268888';\n        $arrOrderDetail['servicePhone'] = '021-22268888';\n        $arrOrderDetail['fetcherNote'] = '取票说明';\n        // 拼装取票信息\n        $arrOrderDetail['fetcherInfo'] = array();\n        $arrOrderDetail['fetcherInfo'][] = array('k' => '取票人', 'v' => $v['FetcherName'], 's' => '');\n        $arrOrderDetail['fetcherInfo'][] = array('k' => '联系电话', 'v' => $this->maskString($v['UserInfo']['ContactMobile']), 's' => '');\n        $arrOrderDetail['fetcherInfo'][] = array('k' => strtr($v['FetcherPaper'], array('二代身份证' => '身份证')), 'v' => $this->maskString($v['DisplayFetcherPaperNum']), 's' => '');\n        //不支持退票\n        $arrOrderDetail['refundTicketType'] = 0;\n        \n        // 退票支持判断start\n        $refundParam = array('user_id'=>$response['Order']['UserInfo']['UserID'],'order_number'=>$arrOrderDetail['orderNumber']);\n        $canRefund = $this->_objOrderApi->isCanRefundTicket($refundParam);\n        $refundTicketList = array();\n        if ($canRefund['code'] == 1) {\n        	if ($canRefund['return']['refund_ticket_type']==1 and isset($canRefund['return']['can_refund_ticket_item'])){\n        		$refundTicketList = $canRefund['return']['can_refund_ticket_item'];\n        		$arrOrderDetail['refundTicketType'] = 1;\n        	}else{\n        		$arrOrderDetail['refundTicketType'] = 2;\n        	}\n        \n        } else {\n        	if ($canRefund['stationCanRefund'] == 1) {\n        		$arrOrderDetail['refundTicketType'] = 0;\n        		$arrOrderDetail['stationCanRefund'] = 1; //车站支持退票\n        		$arrOrderDetail['refundErrorMessage'] = $canRefund['message']; // 无法退票原因\n        	} else {\n        		$arrOrderDetail['refundTicketType'] = 0;\n        	}\n        }\n        // 退票支持判断end\n        \n        $arrSeatNumber = array();\n        foreach ($v['TicketInfo']['TicketItem'] as $k1 => $v1) {\n\n            $ticketInfo['ticketId'] = $v1['TicketId'];\n            $ticketInfo['identityName'] = $v1['IdentityName'];\n            $ticketInfo['identityType'] = strtr($v1['IdentityType'], array('二代身份证' => '身份证'));\n            $ticketInfo['identityCode'] = $this->maskString($v1['DisplayIdentityCode']);\n            $ticketInfo['ticketPrice'] = $v1['TicketPrice']*1;\n            $ticketInfo['ticketType'] = \"成人票\";\n            $ticketInfo['canRefund'] = in_array($ticketInfo['ticketId'],$refundTicketList) ? 1 :
Package.php');\n		$objServicePackage = new servicePackage();\n		$arrData['order_site'] = $busInfo['book_info']['order_site'];\n		$arrData['service_fee'] = intval($busInfo['book_info']['service_fee']);\n		$arrServicePackageConfig = $objServicePackage->getServicePackage($arrData);\n		$arrData['service_fee'] = empty($arrServicePackageConfig) ? 0 : intval($busInfo['book_info']['service_fee']);\n	\n		//区分礼品卡与保险整合\n		if ($arrData['selectServicePackage']) {\n			$arrServicePackageConfigTy = $objServicePackage->getServicePackageTy($busInfo);\n		} elseif ($arrData['selectServicePackage2']) {\n			$arrServicePackageConfigItem = $objServicePackage->getServicePackageItem($busInfo,$arrData);\n		}\n\n		// 计算订单总金额 = 票价*乘客数 + 乘客数*（服务费/支付手续费/0） + 保险*乘客数\n\n\n		//多余乘客是否要参加活动 【前台点击‘继续预定’】传值\n		$isJoinActivity = intval($arrData['isJoinActivity']);\n		if($isJoinActivity ==1){\n			//判断当前车次是否有活动\n			if ($arrOrder['OrderChannel'] == 'ctrip') {\n				$hasBusActive = $objApi->hasBusActive($busInfo, $ticketDate);\n				if ($hasBusActive['code'] == 1) {\n\n					$arrOrder['VendorMark'] = $busInfo['other']['supplier'];\n					//活动开始时间与结束时间和当前活动是否停止并且有活动类型\n					$returnUseValue = $objApi->useVendorActivityStock($fromCityName, $toCityName, $fromStationName, $busNumber, $ticketDate, $ticketCnt);\n					if ($returnUseValue['code']==1) {\n						//活动下单提交参数\n						$arrOrder['VendorReturnPrice'] = $hasBusActive['return'];\n						$arrOrder['VendorReturnType'] = $busInfo['other']['markting']['return_type'];\n						$arrOrder['VendorActivityId'] = $busInfo['other']['markting']['vendor_activity_id'];\n					}else{\n						return $returnUseValue;\n					}\n				}\n			}\n		}\n\n\n\n		\n		$busInfo['utmSource'] = $utmSource;\n		$busInfo['client'] = strpos($arrData['partner'],'h5') === false ? 'ctrip_app' : 'ctrip_h5';\n		$busInfo['clientId'] = empty($arrData['head']['cid']) ? \"\" : $arrData['head']['cid'];\n		$productPackage = XConfig::getProductPackage($busInfo);\n\n		  \n        if($productPackage[$productPackageId] && $productPackage[$productPackageId]['productType'] != 'newuser'){\n            $productPackage =  $productPackage[$productPackageId];\n            $busInfo['full_price_old'] = $busInfo['full_price'];\n            $busInfo['full_price'] = $productPackage['packagePrice'];\n\n	        if($productPackage['productType']=='none'){\n		        //非套餐票儿童半价票\n		        if ($identityTypeChild == 2) {\n			        $totalTicketFee = $busInfo['full_price_old'] * ($ticketCnt - $ChildCnt) + ($busInfo['half_price']) * $ChildCnt;\n			        $arrOrder['TicketFee1']=$totalTicketFee;\n		        }else{\n			        $totalTicketFee = $busInfo['full_price_old'] * $ticketCnt;\n			        $arrOrder['TicketFee2']=$totalTicketFee;\n		        }\n	        }else{\n		        $totalTicketFee = $busInfo['full_price_old'] * $ticketCnt;\n		        $arrOrder['TicketFee3']=$totalTicketFee;\n	        }\n        }else{\n	        if ($identityTypeChild == 2) {\n		        $totalTicketFee = $busInfo['full_price'] * ($ticketCnt - $ChildCnt) + ($busInfo['half_price']) * $ChildCnt;\n		        $arrOrder['TicketFee4']=$totalTicketFee;\n	        }else{\n		        $totalTicketFee = $busInfo['full_price'] * $ticketCnt;\n		        $arrOrder['TicketFee5']=$totalTicketFee;\n	        }\n           \n        }\n\n\n		$serviceFee = 0;\n		$arrOrder['ServiceFee'] = 0; // 服务费\n		$arrOrder['PayCounterFee'] = 0; // 支付手续费\n		$arrOrder['TravelTicketFee'] = 0; // 礼品卡金额\n		$arrOrder['InsuranceFee'] = 0; //保险费用\n\n\n\n\n\n		if(isset($arrData['selectServicePackage'])){\n\n			$servicePackage = $objServicePackage->resultFileterById($arrServicePackageConfigTy,$arrData['selectServicePackage']);\n\n			if($servicePackage['serviceId']=='2' || $servicePackage['serviceId']=='3'){\n				$arrOrder['InsuranceFee'] = $servicePackage['amount'] * $ticketCnt;\n			}\n\n			if($servicePackage['serviceId']=='1'){\n				$arrOrder['AcceptFreeInsurance'] = 1;\n			}\n\n			if($servicePackage['serviceId']=='0'){\n\n			}\n\n			$arrOrder['ServiceFee'] = intval($ar
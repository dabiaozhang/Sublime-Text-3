$arrLoginUserInfo;\n		}\n	}\n\n	private function getOrderNumberByRef($strRef) {\n		require_once 'bus/control/order/class.orderNo.php';\n		$objOrderNo = new orderNo();\n		if ($strRef == 'ctrip.h5') {\n			return $objOrderNo->getCtripOrderNumber();\n		} else {\n			return $objOrderNo->getOrderNumber();\n		}\n	}\n\n	private function getOrderChannelByRef($strRef) {\n		if ($strRef == 'ctrip.h5') {\n			return 'ctrip';\n		} else {\n			return 'tieyou';\n		}\n	}\n	/**\n	 * 选择使用优惠券\n	 *\n	 * @param array $arrData\n	 * @param array $arrOrder\n	 * @return array\n	 */\n	private function selectUseCoupon($arrData, &$arrOrder) {\n		// 判断是否使用了优惠券（首先判断是否是携程渠道，在判断是否使用优惠券）\n		if ($arrData['ref'] == 'ctrip.h5') {\n			if (!empty($arrData['couponCode'])) {\n				// 校验客户端类型\n				if (!in_array($arrData['useCouponClientType'], array(1, 2))) {\n					$arrReturn['code']    = 0;\n					$arrReturn['message'] = '不支持使用优惠券';\n					$arrReturn['return']  = '';\n					return $arrReturn;\n				}\n\n				require_once('bus/control/api/class.couponApi.php');\n				$objCouponApi = new couponApi();\n				$arrCoupon = $objCouponApi->getUserCouponDetail($arrOrder['UserInfo']['UserID'], $arrData['couponCode']);\n				// 校验使用优惠券\n				$arrCheckUserCoupon = $objCouponApi->checkUseCoupon($arrCoupon, $arrOrder);\n				if ($arrCheckUserCoupon['code'] == 0) {\n					return $arrCheckUserCoupon;\n				}\n\n				// 解析使用站点信息\n				$useStation = explode(',', $arrCoupon['UseStation']);\n				$arrOrder['CouponInfo'] = array(\n					'CouponCode' => $arrCoupon['CouponCode'], // 优惠券编号\n					'PromotionID' => $arrCoupon['PromotionID'], // 策略id\n					'UseTerminal' => intval($arrData['useCouponClientType']), // 适用终端 1->APP 2->H5 3->PC\n					'UseStation' => $useStation[0], // 适用站点\n					'CouponPrice' => $arrCoupon['DeductionStrategy'][0]['DeductionAmount'] // 优惠券价格\n				);\n\n				// 适用限制 1->该订单购买了保险或礼品可 2->单纯购票\n				if ($arrOrder['TravelTicketFee'] > 0) {\n					$arrOrder['CouponInfo']['Use_limit'] = 1;\n				} else {\n					$arrOrder['CouponInfo']['Use_limit'] = 2;\n				}\n			}\n		}\n\n		$arrReturn['code']    = 1;\n		$arrReturn['message'] = '';\n		$arrReturn['return']  = '';\n		return $arrReturn;\n	}\n	/**\n	 * 掩码数据\n	 * @param  [type] $str [description]\n	 * @return [type]      [description]\n	 */\n	private function maskString($string,$encoding='UTF8'){\n		\n		$start = intval(mb_strlen($string,$encoding)/3);\n		$length = intval(mb_strlen($string,$encoding)/3)+1;\n\n		$replacement = '';\n\n		for ($i=0; $i < $length; $i++) { \n			$replacement .= '*';\n		}\n\n		\n        if ($encoding == null){\n            if ($length == null){\n                return mb_substr($string,0,$start).$replacement;\n            }\n            else{\n                return mb_substr($string,0,$start).$replacement.mb_substr($string,$start + $length);\n            }\n        }\n        else{\n            if ($length == null){\n                return mb_substr($string,0,$start,$encoding).$replacement;\n            }\n            else{\n                return mb_substr($string,0,$start,$encoding). $replacement. mb_substr($string,$start + $length,mb_strlen($string,$encoding),$encoding);\n            }\n        }\n	}\n	//订单详情出票或抢票状态免责提示(备用)\n	function setOrderTicketChangeTip(){\n		return false;\n	}\n}\n",
